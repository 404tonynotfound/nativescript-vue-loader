      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "peerDependencies": {
    "nativescript-vue-template-compiler": "^1.3.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.0",
    "eslint-plugin-vue-libs": "^2.1.0",
    "husky": "^0.15.0-rc.9",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "nativescript-vue-template-compiler": "^1.3.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12"
  }
}

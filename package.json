{
  "name": "nativescript-vue-loader",
  "version": "1.0.0",
  "description": "Vue single-file component loader for Webpack",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rigor789/nativescript-vue-loader.git"
  },
  "keywords": [
    "nativescript-vue",
    "webpack",
    "loader"
  ],
  "files": [
    "lib"
  ],
  "author": "Igor Randjelovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nativescript-vue/nativescript-vue-loader/issues"
  },
  "homepage": "https://github.com/nativescript-vue/nativescript-vue-loader",
  "scripts": {
    "lint": "eslint lib test --fix",
    "test": "jest --env node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
